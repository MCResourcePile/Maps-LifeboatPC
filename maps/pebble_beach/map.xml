<?xml version="1.0"?>
<map proto="1.3.3">
<name>Pebble Beach</name>
<version>1.0.1</version>
<objective>Eliminate everyone on the other team within 5 minutes!</objective>
<gamemode>blitz</gamemode>
<authors>
    <author uuid="130be12a-f3b9-4b7d-ad39-7b45e84d3d0f"/> <!-- Veiyl -->
</authors>
<contributors>
    <contributor uuid="e5eea785-51c1-40fe-a2d0-f83d4c035cd9" contribution="XML"/> <!-- pikpikmerchant -->
</contributors>
<timelock>on</timelock>
<blitz>
    <time>5m</time>
    <lives>1</lives>
</blitz>
<teams>
    <team color="dark red" max="12">Red</team>
    <team color="blue" max="12">Blue</team>
</teams>
<regions>
    <apply block="deny-all">
        <rectangle min="-oo,-oo" max="oo,oo"/>
    </apply>
</regions>
<kits>
    <kit name="spawn">
        <item slot="0">stone sword</item>
        <item slot="1">bow</item>
        <item slot="2" amount="20">arrow</item>
        <item slot="8" amount="32">bread</item>
    </kit>
    <kit name="red" parents="spawn">
        <helmet color="993333" unbreakable="true">leather helmet</helmet>
        <chestplate color="993333" unbreakable="true">leather chestplate</chestplate>
        <boots color="993333" unbreakable="true">leather boots</boots>
    </kit>
    <kit name="blue" parents="spawn">
        <helmet color="334CB2" unbreakable="true">leather helmet</helmet>
        <chestplate color="334CB2" unbreakable="true">leather chestplate</chestplate>
        <boots color="334CB2" unbreakable="true">leather boots</boots>
    </kit>
</kits>
<spawns>
    <spawn team="red" kit="red" yaw="145">
        <block>16.5,16,42.5</block>
    </spawn>
    <spawn team="blue" kit="blue" yaw="-40">
        <block>-15.5,16,-41.5</block>
    </spawn>
    <default yaw="90">
        <cylinder base="-41.5,28,0.5" radius="1" height="0"/>
    </default>
</spawns>
<friendlyfirerefund>on</friendlyfirerefund>
<itemremove>
    <item>bread</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather boots</item>
</itemremove>
<toolrepair>
    <tool>stone sword</tool>
    <tool>bow</tool>
</toolrepair>
</map>
